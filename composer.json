{
    "name": "saleem-hadad/larecipe",
    "description": "Generate gorgeous recipes for your Laravel applications using MarkDown",
    "keywords": ["laravel", "markdown", "docs", "documentation", "api"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/saleem-hadad/larecipe/issues",
        "source": "https://github.com/saleem-hadad/larecipe"
    },
    "authors": [
        {
            "name": "Saleem Hadad",
            "email": "saleem@binary-torch.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "erusev/parsedown-extra": "^0.7.1",
        "symfony/dom-crawler": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "laravel/framework": "~5.5.0|~5.6.0",
        "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "BinaryTorch\\LaRecipe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinaryTorch\\LaRecipe\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "BinaryTorch\\LaRecipe\\LaRecipeServiceProvider"
            ]
        }
    }
}
